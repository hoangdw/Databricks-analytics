{
  "datasets": [
    {
      "name": "4e57cd98",
      "displayName": "Aggregate Sales Summary Report",
      "queryLines": [
        "-- Total sales\r\n",
        "SELECT SUM(sales_amount) AS total_sales FROM workspace.databricks_analytics.fact_sales;"
      ]
    },
    {
      "name": "b19b1fd9",
      "displayName": "Unique Customer Count Summary",
      "queryLines": [
        "-- Total customers\r\n",
        "SELECT COUNT(DISTINCT customer_key) AS total_customers FROM workspace.databricks_analytics.dim_customers;"
      ]
    },
    {
      "name": "aa7731ca",
      "displayName": "Unique Product Count Summary",
      "queryLines": [
        "-- Total products\r\n",
        "SELECT COUNT(DISTINCT product_key) AS total_products FROM workspace.databricks_analytics.dim_products;"
      ]
    },
    {
      "name": "9d885a74",
      "displayName": "Total Unique Orders Count",
      "queryLines": [
        "-- Total orders\r\n",
        "SELECT COUNT(DISTINCT order_number) AS total_orders FROM workspace.databricks_analytics.fact_sales;"
      ]
    },
    {
      "name": "db0533b6",
      "displayName": "Sales Performance Overview",
      "queryLines": [
        "-- Sales by product\r\n",
        "SELECT product_key, SUM(sales_amount) AS sales_by_product FROM workspace.databricks_analytics.fact_sales GROUP BY product_key;"
      ]
    },
    {
      "name": "b4841175",
      "displayName": "Sales Insights by Product Category",
      "queryLines": [
        "-- Sales by category\r\n",
        "SELECT p.category, SUM(f.sales_amount) AS sales_by_category\r\n",
        "FROM workspace.databricks_analytics.fact_sales f\r\n",
        "JOIN workspace.databricks_analytics.dim_products p ON f.product_key = p.product_key\r\n",
        "GROUP BY p.category;"
      ]
    },
    {
      "name": "494ed780",
      "displayName": "Order Trends Analysis Over Time",
      "queryLines": [
        "-- Orders over time\r\n",
        "SELECT order_date, COUNT(DISTINCT order_number) AS orders_count\r\n",
        "FROM workspace.databricks_analytics.fact_sales\r\n",
        "GROUP BY order_date\r\n",
        "ORDER BY order_date;"
      ]
    }
  ],
